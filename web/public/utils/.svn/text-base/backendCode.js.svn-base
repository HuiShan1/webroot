const SUCCESS = '88888'
const NOT_FOUND = '50001'
module.exports = {
  getCodeInfo: (body, lang) => {
    if (!body) {
      return codes['apiLost'][(lang || 'zh-cn')]
    }
    if (!codes[body.code]) {
      return '异常码' + body.code ? body.code : ''
    }
    var codeStr = (codes[body.code].showCode?(body.code + ':'):'') + codes[body.code][(lang || 'zh-cn')]
    return codeStr
  },
  success: SUCCESS,
  notFound: NOT_FOUND
}
